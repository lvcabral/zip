{
	"name": "@lvcabral/zip",
	"version": "1.3.5",
	"description": "Zip backend for ZenFS (with case fold mode option)",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"filesystem",
		"node",
		"storage",
		"zip"
	],
	"type": "module",
	"homepage": "https://github.com/lvcabral/zip",
	"author": "James Prevett <jp@jamespre.dev> (https://jamespre.dev)",
	"contributors": [
		"Marcelo Lv Cabral <marcelo@lvcabral.com>"
	],
	"license": "LGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lvcabral/zip.git"
	},
	"bugs": {
		"url": "https://github.com/lvcabral/zip/issues"
	},
	"engines": {
		"node": ">= 20"
	},
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"files": [
		"dist",
		"license.md"
	],
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint src",
		"test": "npx zenfs-test -abcf",
		"build": "tsc -p tsconfig.json",
		"build:docs": "typedoc",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"c8": "^10.1.3",
		"eslint": "^9.12.0",
		"globals": "^16.0.0",
		"prettier": "^3.2.5",
		"tsx": "^4.19.1",
		"typedoc": "^0.28.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.8.1"
	},
	"peerDependencies": {
		"@zenfs/core": "^2.4.2",
		"kerium": "^1.3.4",
		"memium": "^0.3.8",
		"utilium": "^2.5.0"
	},
	"dependencies": {
		"fflate": "^0.8.2"
	}
}
